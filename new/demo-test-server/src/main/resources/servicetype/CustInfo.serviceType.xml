<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="CustInfo" kind="business" longname="客户服务" package="cn.sunline.edsp.demo.test.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="createCustInfo" name="createCustInfo" longname="创建客户信息服务">
        <interface>
            <input packMode="true">
                <field id="custname" type="BasicType.U_CUSTNAME" required="false" multi="false" array="false" longname="客户姓名" ref="Dict.CustInfo.custname"/>
                <field id="idcardno" type="BasicType.U_IDCARDNO" required="false" multi="false" array="false" longname="身份证号码" ref="Dict.CustInfo.idcardno"/>
                <field id="telenum" type="BasicType.U_TELENUM" required="false" multi="false" array="false" longname="电话号码" ref="Dict.CustInfo.telenum"/>
                <field id="addr" type="BasicType.U_ADDR" required="false" multi="false" array="false" longname="住址" ref="Dict.CustInfo.addr"/>
                <field id="busistatus" type="BasicType.U_BUSISTATUS" required="false" multi="false" array="false" longname="业务状态" ref="Dict.CustInfo.busistatus"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="custid" type="BasicType.U_CUSTID" required="false" multi="false" array="false" longname="客户号" ref="Dict.CustInfo.custid"/>
                <field id="busistatus" type="BasicType.U_BUSISTATUS" required="false" multi="false" array="false" longname="业务状态" ref="Dict.CustInfo.busistatus"/>
                <field id="custserialnum" type="BasicType.U_SERIALNUM" required="false" multi="false" array="false" longname="客户业务流水号" ref="Dict.CustInfo.custserialnum"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
