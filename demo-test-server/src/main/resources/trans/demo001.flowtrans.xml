<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="demo001" longname="apltdemo测试" kind="create" package="cn.sunline.edsp.demo.test.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.edsp.demo.test.trans.intf">
        <input packMode="true">
            <field id="custname" type="BasicType.U_CUSTNAME" required="false" multi="false" array="false" longname="客户姓名" ref="Dict.CustInfo.custname"/>
            <field id="idcardno" type="BasicType.U_IDCARDNO" required="false" multi="false" array="false" longname="身份证号码" ref="Dict.CustInfo.idcardno"/>
            <field id="telenum" type="BasicType.U_TELENUM" required="false" multi="false" array="false" longname="电话号码" ref="Dict.CustInfo.telenum"/>
            <field id="addr" type="BasicType.U_ADDR" required="false" multi="false" array="false" longname="住址" ref="Dict.CustInfo.addr"/>
            <field id="busistatus" type="BasicType.U_BUSISTATUS" required="false" multi="false" array="false" longname="业务状态" ref="Dict.CustInfo.busistatus"/>
            <field id="opentdate" type="BasicType.U_OPENDATE" required="false" multi="false" array="false" longname="开户日期" ref="Dict.AccountInfo.opentdate"/>
            <field id="closedate" type="BasicType.U_CLOSEDATE" required="false" multi="false" array="false" longname="销户日期" ref="Dict.AccountInfo.closedate"/>
            <field id="pwd" type="BasicType.U_PWD" required="false" multi="false" array="false" longname="密码" ref="Dict.AccountInfo.pwd"/>
            <field id="accountstatus" type="AtEnumType.E_ACCOUNTSTATUS" required="false" multi="false" array="false" longname="账户状态" ref="Dict.AccountInfo.accountstatus"/>
            <field id="waiterno" type="BasicType.U_WAITERID" required="false" multi="false" array="false" longname="业务员编号" ref="Dict.Common.waiterno"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="custid" type="BasicType.U_CUSTID" required="false" multi="false" array="false" longname="客户号" ref="Dict.CustInfo.custid"/>
            <field id="busistatus" type="BasicType.U_BUSISTATUS" required="false" multi="false" array="false" longname="业务状态" ref="Dict.CustInfo.busistatus"/>
            <field id="custserialnum" type="BasicType.U_SERIALNUM" required="false" multi="false" array="false" longname="客户业务流水号" ref="Dict.CustInfo.custserialnum"/>
            <field id="accountno" type="BasicType.U_ACCOUNTNO" required="false" multi="false" array="false" longname="账号" ref="Dict.AccountInfo.accountno"/>
            <field id="accountserialnum" type="BasicType.U_BUSISTATUS" required="false" multi="false" array="false" longname="账户业务流水号" ref="Dict.AccountInfo.accountserialnum"/>
            <field id="accountstatus" type="AtEnumType.E_ACCOUNTSTATUS" required="false" multi="false" array="false" longname="账户状态" ref="Dict.AccountInfo.accountstatus"/>
        </output>
        <property packMode="true">
            <field id="custid" type="BasicType.U_CUSTID" required="false" multi="false" array="false" longname="客户号" ref="Dict.CustInfo.custid"/>
            <field id="custserialnum" type="BasicType.U_SERIALNUM" required="false" multi="false" array="false" longname="客户业务流水号" ref="Dict.CustInfo.custserialnum"/>
            <field id="busistatus" type="BasicType.U_BUSISTATUS" required="false" multi="false" array="false" longname="业务状态" ref="Dict.CustInfo.busistatus"/>
        </property>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service mappingToProperty="true" serviceName="CustInfo.createCustInfo" id="createCustInfo" longname="创建客户信息">
            <in_mappings by_interface="true">
                <mapping src="addr" dest="addr" by_interface="true" on_top="true"/>
                <mapping src="busistatus" dest="busistatus" by_interface="true" on_top="true"/>
                <mapping src="custname" dest="custname" by_interface="true" on_top="true"/>
                <mapping src="idcardno" dest="idcardno" by_interface="true" on_top="true"/>
                <mapping src="telenum" dest="telenum" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="busistatus" dest="busistatus" by_interface="true" on_top="true"/>
                <mapping src="custid" dest="custid" by_interface="true" on_top="true"/>
                <mapping src="custserialnum" dest="custserialnum" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
        <service serviceName="AccountInfo.createAccountInfo" id="createAccountInfo" longname="创建账户号">
            <in_mappings by_interface="true">
                <mapping src="accountstatus" dest="accountstatus" by_interface="true" on_top="true"/>
                <mapping src="closedate" dest="closedate" by_interface="true" on_top="true"/>
                <mapping src="custid" dest="custid" by_interface="true" on_top="true"/>
                <mapping src="opentdate" dest="opentdate" by_interface="true" on_top="true"/>
                <mapping src="pwd" dest="pwd" by_interface="true" on_top="true"/>
                <mapping src="waiterno" dest="waiterno" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="accountno" dest="accountno" by_interface="true" on_top="true"/>
                <mapping src="accountserialnum" dest="accountserialnum" by_interface="true" on_top="true"/>
                <mapping src="accountstatus" dest="accountstatus" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping by_interface="true">
        <mapping src="busistatus" dest="busistatus" by_interface="true" on_top="true"/>
        <mapping src="custid" dest="custid" by_interface="true" on_top="true"/>
        <mapping src="custserialnum" dest="custserialnum" by_interface="true" on_top="true"/>
    </outMapping>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
